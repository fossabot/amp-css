{
  "name": "amp-css",
  "version": "1.0.0",
  "description": "Provides a binary to compile SCSS files and validate them for AMP.",
  "bin": {
    "amp-css": "bin/amp-css"
  },
  "repository": {
    "type": "git",
    "url": "git@git.saitho.me:saitho/amp-css.git"
  },
  "author": "Mario Lubenka",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc && cp ./dist/amp-css.js ./bin/amp-css"
  },
  "dependencies": {
    "amphtml-validator": "^1.0.23",
    "meow": "^5.0.0",
    "node-sass": "^4.11.0",
    "node-sass-tilde-importer": "git+https://github.com/saitho/node-sass-tilde-importer.git"
  },
  "devDependencies": {
    "@types/amphtml-validator": "^1.0.1",
    "@types/meow": "^5.0.0",
    "@types/node-sass": "^4.11.0",
    "typescript": "^3.7.3"
  }
}
